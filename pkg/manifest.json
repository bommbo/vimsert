{
  "manifest_version": 2,

  "name": "Vimsert",
  "version": "0.0.1",

  "description": "Vimsert allows you to edit any textarea in a vim-mode Ace code editor modal.",
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.js", "inc/ace.js", "inc/keybinding-vim.js", "inc/theme-solarized_dark.js", "vimsert.min.js"]
    }
  ]
}
